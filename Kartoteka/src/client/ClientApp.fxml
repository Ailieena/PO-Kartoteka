<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <HBox>
      <children>
        <AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: #006400;">
          <children>
            <HBox fx:id="handTop" layoutX="100.0" prefHeight="100.0" prefWidth="400.0" />
            <VBox fx:id="handLeft" layoutY="100.0" prefHeight="400.0" prefWidth="100.0" />
            <VBox fx:id="handRight" layoutX="500.0" layoutY="100.0" prefHeight="400.0" prefWidth="100.0" />
            <HBox fx:id="handBottom" layoutX="100.0" layoutY="500.0" prefHeight="100.0" prefWidth="400.0" />
            <HBox fx:id="handTable" layoutX="200.0" layoutY="250.0" prefHeight="100.0" prefWidth="200.0" />
          </children>
        </AnchorPane>

        <AnchorPane prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: #6b8e23;">
          <children>
            <Button fx:id="onPassBtn" alignment="TOP_CENTER" layoutX="57.0" layoutY="71.0" mnemonicParsing="false" onAction="#onPass" prefHeight="25.0" prefWidth="69.0" text="Pass" textAlignment="CENTER" />
            <Button fx:id="onGetCardBtn" alignment="TOP_CENTER" layoutX="57.0" layoutY="40.0" mnemonicParsing="false" onAction="#onGetCard" text="Weź kartę" textAlignment="CENTER" />
            <Button fx:id="buttonExit" alignment="BOTTOM_RIGHT" layoutX="78.0" layoutY="518.0" mnemonicParsing="false" onAction="#exitApp" text="Exit" textAlignment="CENTER" />
          </children>
        </AnchorPane>
      </children>
    </HBox>
  </children>
</VBox>
